/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.one{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.two{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

#fix_user{
  width: 100%;
  height: 100%;
  background: none;
  position: fixed;
  top: 0;
  z-index: 15;
}
#fix_mobile{
 width: 100%;
 height: 100%;
 background: none;
 position: fixed;
 top: 0;
 z-index: 14;
}

.none{
  width: 50%;
  padding-top: 20%;
  margin:0 auto;
  box-sizing: border-box;
}
.none image{
  width: 100%;
  display: block;
}
.none_v{
  width: 100%;
  text-align:center;
  color: #999;
  font-size:26rpx;
  font-weight: 700;
  letter-spacing: 1px;
  margin-top:20rpx;
}