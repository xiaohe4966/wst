/* pages/u_address/u_address.wxss */
page{
  background: #eaebef;
}
.app{
  width: 100%;
  padding: 0 25rpx;
  box-sizing: border-box;
}
.title{
  width: 100%;
  padding: 40rpx 15rpx 15rpx;
  box-sizing: border-box;
  color: #000;
  font-size: 26rpx;
  font-weight: 700;
}
.inp{
  width: 100%;
  height: 80rpx;
  padding-left: 15rpx;
  box-sizing: border-box;
  color: #000;
  font-size: 28rpx;
  background: white;
  border-radius: 5rpx;
}
.picker{
 width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 15rpx;
  box-sizing: border-box;
  color: #000;
  font-size: 28rpx;
  background: white;
  border-radius: 5rpx;
}
.pla{
  color: #999;
  font-size: 28rpx;
}
.btn{
  width: 100%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  background: #306dff;
  color: white;
  font-size: 30rpx;
  font-weight: 700;
  margin-top: 50rpx;
  border-radius: 10rpx;
  box-shadow: #a4b1d2 0 10px 13px;
}
.ts{
  width: 100%;
  margin: 20rpx 0;
  color: #000;
  font-size: 28rpx;
  font-weight: 700;
}