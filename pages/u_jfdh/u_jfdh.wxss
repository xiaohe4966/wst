/* pages/u_jfdh/u_jfdh.wxss */
page{
  background: #eaebef;
}
.app{
  width: 100%;
  padding: 30rpx 25rpx;
  box-sizing: border-box;
}
.head{
  width: 100%;
  height: 180rpx;
  padding: 40rpx 0 0 120rpx;
  box-sizing: border-box;
  background: white;
  position: relative;
}
.head_img1{
  width: 130rpx;
  position: absolute;
  left: -23rpx;
  bottom: 0;
}
.head_img2{
  width: 230rpx;
  position: absolute;
  right: -22rpx;
  top: 0;
}
.head_v{
  width: 100%;
}
.head_title{
  color: #ec9915;
  font-size: 32rpx;
  font-weight: 700;
  margin-bottom: 15rpx;
}
.head_jf{
  color: #ec9915;
  font-size: 26rpx;
}
.con{
  width: 100%;
}
.list{
  width: 100%;
  padding: 30rpx 0;
  border-bottom: 1px solid #939599;
  display: flex;
}
.list_left{
  width: 35%;
}
.list_left image{
  width: 220rpx;
  height: 220rpx;
  display: block;
  border-radius: 6rpx;
}
.list_right{
  width: 65%;
  height: 220rpx;
  position: relative;
}
.list_name{
  width: 100%;
  color: #3f4044;
  font-size: 28rpx;
  font-weight: 700;
  margin-bottom: 15rpx;
}
.list_con{
  width: 100%;
  color: #999;
  font-size: 26rpx;
}
.list_btn{
  width: 100%;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background: #306dff;
  border-radius: 10rpx;
  color: white;
  box-shadow: #a4b1d2 0 2px 10px;
  font-size: 26rpx;
  position: absolute;
  bottom: 0;
  z-index: 11;
}
.list_btn text{
  font-size: 30rpx;
  font-weight: 700;
  margin-right: 10rpx;
}