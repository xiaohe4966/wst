/* pages/i_con/i_con.wxss */
page{
  background: #eaebef;
}
.app{
  width: 100%;
  padding: 25rpx;
  box-sizing: border-box;
}
.content{
  width: 100%;
  padding: 28rpx;
  box-sizing: border-box;
  border: 1px solid #939599;
  border-radius: 5rpx;
  color: #000;
  font-size: 28rpx;
}
.pla{
  color: #a5a7aa;
  font-size: 28rpx;
}
.btn{
  width: 100%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  background: #306dff;
  color: white;
  font-size: 30rpx;
  font-weight: 700;
  margin-top: 90rpx;
  border-radius: 10rpx;
  box-shadow: #a4b1d2 0 10px 13px;
}